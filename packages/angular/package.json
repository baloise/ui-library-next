{
  "name": "@baloise/ui-library-next-angular",
  "version": "1.19.5",
  "description": "The UI-Libary is an open source project for styling awesome web applications that follow the Baloise corporate styling guidelines.",
  "repository": {
    "type": "git",
    "url": "https://github.com/baloise/ui-library-next.git"
  },
  "homepage": "https://ui-library-next.vercel.app",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && ng-packagr -p package.json",
    "clean": "rimraf dist"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    },
    "whitelistedNonPeerDependencies": [
      "@baloise/ui-library-next",
      "@angular/common",
      "@angular/core",
      "@angular/forms"
    ]
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@baloise/ui-library-next": "^1.19.5",
    "@baloise/ui-library-next-vue": "^1.19.5",
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "@angular/common": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0"
  },
  "gitHead": "722ef389df9b49212894042493cb203eb8fd8f09",
  "devDependencies": {
    "@angular/common": "^10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@angular/core": "^10.2.0",
    "@angular/forms": "^10.2.0",
    "ng-packagr": "^10.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  }
}
