{
  "name": "@baloise/ui-library-next",
  "version": "0.3.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "The UI-Libary is an open source project for styling awesome web applications that follow the Baloise corporate styling guidelines.",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ui-library-next/ui-library-next.js",
  "files": [
    "dist/",
    "src/styles/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs --es5",
    "build:docs": "stencil build --docs --es5 --config stencil.docs.config.ts",
    "serve": "stencil build -docs --dev --watch --serve --docs-json www/components.json",
    "serve:ie": "stencil build --es5 --dev --watch --serve",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "bulma": "^0.9.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/angular-output-target": "0.0.5",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@stencil/vue-output-target": "0.0.3",
    "@types/autoprefixer": "^9.7.2",
    "autoprefixer": "^10.0.1"
  },
  "gitHead": "b167aad662bd9c34eb0b5d894b350c69370759a2"
}
